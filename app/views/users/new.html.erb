<div class="mx-auto md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">New user</h1>

  <%= form_for(@user) do |form| %>
    <%= render 'devise/registrations/form', form: form, user: @user %>
    <div class="actions m-1">
      <%= form.submit "Create", class: "rounded-lg py-3 px-5 bg-green-600 text-white inline-block font-medium cursor-pointer w-full" %>
    </div>
  

  <%= link_to "Back to users", users_path, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  <% end %>
</div>
