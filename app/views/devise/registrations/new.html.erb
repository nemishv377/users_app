<div>
<h1 class="font-bold text-4xl">Sign up</h1>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |form| %>
  
    
  <%= render "form", user: @user, form: form %>

  <div class="actions m-1">
    <%= form.submit "Sign up", class: "rounded-lg py-3 px-5 bg-green-600 text-white inline-block font-medium cursor-pointer w-full" %>
  </div>

<% end %>
  <div style="color:blue; margin:10px;">
    <%= render "devise/shared/links" %>
  </div>
</div>